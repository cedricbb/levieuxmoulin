{
  "expo": {
    "name": "Le Vieux Moulin",
    "slug": "levieuxmoulin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "statusBar": {
      "style": "auto",
      "hidden": true,
      "translucent": true
    },
    "ios": {
      "supportsTablet": true,
      "statusBarStyle": "auto"
    },
    "android": {
      "package": "com.cedricbillard.levieuxmoulin",
      "statusBar": {
        "translucent": true,
        "backgroundColor": "transparent",
        "hidden": true
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d98a17a8-3273-4247-a212-5f40393511e4"
      }
    }
  }
}
